	EXP(-("Rate of decline in CH4 per kg crop 1/y")
	 * ( Time - 1980))
	*IF THEN ELSE(Time>2022, EXP(-("Extra rate of decline in CH4 pr kg crop after 2022 1/y"\
		)
	 * ( Time - 2022)), 1)
	~	1
	~		|

